<h1>Find Your Way Around</h1>

<!DOCTYPE html>
    <html>
    <body>


    <div id="googleMap" style="width: 100%; height: 400px;"></div>

    <script>
        function myMap() {
            var mapProp = {
                center: new google.maps.LatLng(43.038902, -87.906471),
                zoom: 10,
            };
            var map = new google.maps.Map(document.getElementById("googleMap"), mapProp);
        }
    </script>

    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAImQTSw-wJ1gR9QM8Ek0nH4OkPMlE26j4&callback=myMap"></script>


    </body>
    </html>

